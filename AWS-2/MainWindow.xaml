<Window x:Class="AWS_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AWS_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="600">
    <Grid Background="#1A1A1D">
        <Label Content="Amazon Web Services" Foreground="#C3073F" FontSize="25" HorizontalAlignment="Left" Margin="150,25,0,0" VerticalAlignment="Top" Width="300" FontWeight="Bold" HorizontalContentAlignment="Center" Height="40"/>
        <Button Click="NewPictureServices" Width="100" Height="35" BorderThickness="2" BorderBrush="#950740" Background="#1A1A1D" Content="Picture" Foreground="White" FontSize="20" Margin="-300,0,0,0" FontWeight="Medium"/>
        <Button Click="NewTextServices" Width="100" Height="35" BorderThickness="2" BorderBrush="#950740" Background="#1A1A1D" Content="Text" Foreground="White" FontSize="20" Margin="300,0,0,0" FontWeight="Medium"/>
        <Line X1="0" Y1="65" X2="0" Y2="0" StrokeThickness="5" Stroke="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>

    </Grid>
</Window>
